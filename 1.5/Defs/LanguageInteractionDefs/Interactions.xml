 <Defs>
 <InteractionDef>
    <defName>LanguageInteractionSuccess</defName>
    <label>language interaction sucess</label>
    <workerClass>InteractionWorker</workerClass>
    <symbol>Things/Mote/SpeechSymbols/LinguisticInteractionSuccess</symbol>
    <recipientThought>LinguisticallyUnderstood</recipientThought>
    <logRulesInitiator>
      <rulesStrings>
        <li>r_logentry->[INITIATOR_nameDef] [complimented] [RECIPIENT_nameDef].</li>

        <li>complimented(p=3)->spoke in a shared language with</li>
      </rulesStrings>
    </logRulesInitiator>
  </InteractionDef>
  
 <InteractionDef>
    <defName>LanguageInteractionFail</defName>
    <label>language interaction fail</label>
    <workerClass>InteractionWorker</workerClass>
    <symbol>Things/Mote/SpeechSymbols/LinguisticInteractionFail</symbol>
    <recipientThought>LinguisticallyMisunderstood</recipientThought>
    <logRulesInitiator>
      <rulesStrings>
      <!-- r_logentry->[initiator] tried to [complimented] [Recipient] but failed dut to a language barrier.-->

        <li>r_logentry->[INITIATOR_nameDef] [complimented] [RECIPIENT_nameDef], but failed due to a language barrier.</li>

        <li>complimented(p=3)->tried to speak to</li>
      </rulesStrings>
    </logRulesInitiator>
  </InteractionDef>
  
   <InteractionDef>
    <defName>LanguageLost</defName>
    <label>language lost</label>
    <workerClass>InteractionWorker</workerClass>
    <symbol>Things/Mote/SpeechSymbols/LinguisticInteractionLanguageLost</symbol>
    <logRulesInitiator>
      <rulesStrings>

        <li>r_logentry->[INITIATOR_nameDef] lost the ability to speak [LOSTLANGUAGE].</li>

      </rulesStrings>
    </logRulesInitiator>
  </InteractionDef>
  </Defs>